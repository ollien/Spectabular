{
	"name":"Tabular",
	"description":"Tree-based-tabs in chrome",
	"author":"ollien",
	"version":"0.1",
	"manifest_version":3,
	"icons":{
		"16":"img/icon16.png",
		"48":"img/icon48.png",
		"128":"img/icon128.png"
	},
	"offline_enabled":true,
	"browser_action":{
		"default_icon":{
			"19":"img/icon19.png",
			"38":"img/icon38.png"
		},
		"default_popup":"popup.html"
	},
	"commands":{
		"_execute_browser_action":{
			"suggested_key":{
				"windows":"Ctrl+Shift+E",
				"mac":"Command+Shift+E",
				"chromeos":"Ctrl+Shift+E",
				"linux":"Ctrl+Shift+E"
			}
		}
	},
	"permissions":["tabs","storage","unlimitedStorage"]
	
		
}